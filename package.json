    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-1": "^6.3.13",
    "babel-preset-stage-2": "^6.3.13",
    "chai": "^3.4.1",
    "eslint": "^1.10.3",
    "eslint-config-alvinteh": "^0.1.1",
    "eslint-loader": "^1.2.0",
    "eslint-plugin-react": "^3.14.0",
